#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int rem,num,sum;
    scanf("%d",&num);
    
    rem= num%10;
    
    while(num>=9){
        
        num=num/10;
    }
    
    sum=rem+num; 
    
    printf("%d",sum);
    
    return 0;
}
