#include <stdio.h>

int main()
{
    int num,i,j;
    
    scanf("%d",&num);
    
    for(i=1; i<=num; i++){
        
        for(j=i; j>1; j--){
            
            printf("%d",j);
        }
        for(j=1; j<= (num-i+1); j++)
        {
            
            printf("%d",j);
        }
        printf("\n");
    }

    return 0;
}

