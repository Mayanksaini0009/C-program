#include<stdio.h>
int main ()
{
    int a=9,b;
    scanf("%d",&b);
    for(int i=0; i<b;i++)
    {
       
        for(int j=0;j<=i;j++)
        {
            printf("%d",a); 
        
            
      }  if(i==b-1)
        {
            break;
        }
        else
        {
        
         printf("+"); }
    }
   
}
